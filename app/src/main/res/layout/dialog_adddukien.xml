<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="hust.nhatlx.mysishust.BangDiem_Activity">

    <Button
        android:id="@+id/btnAddCaiThien_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/common_google_signin_btn_text_dark_default"
        android:text="@string/them_mon_du_kien"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout21"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btnAddHocPhanMoi_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/common_google_signin_btn_text_dark_default"
        android:text="@string/them_hoc_phan_moi"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout21"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.555" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:background="@color/colorPrimaryDark"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/linearLayout19"></LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:background="@color/colorPrimaryDark"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/linearLayout20"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="70dp"></LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout21"
        android:layout_width="3dp"
        android:layout_height="70dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="0dp"
        android:background="@color/colorPrimaryDark"
        android:orientation="vertical"
        app:layout_constraintLeft_toRightOf="@+id/btnAddCaiThien_dialog"
        app:layout_constraintRight_toLeftOf="@+id/btnAddHocPhanMoi_dialog"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout19"></LinearLayout>
</android.support.constraint.ConstraintLayout>
